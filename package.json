{
  "name": "pinata-security-cli",
  "version": "0.1.4",
  "description": "AI-powered test coverage analysis and generation tool. Find security blind spots before attackers do.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "pinata": "dist/cli/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pinata-security/pinata.git"
  },
  "homepage": "https://pinata.security",
  "bugs": {
    "url": "https://github.com/pinata-security/pinata/issues"
  },
  "files": [
    "dist",
    "wasm",
    "src/categories/definitions",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/cli/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "benchmark": "vitest run tests/benchmarks/ --reporter=verbose",
    "benchmark:quick": "vitest run tests/benchmarks/scan-benchmark.test.ts --reporter=verbose",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "setup:wasm": "npx tsx scripts/setup-wasm.ts",
    "postinstall": "node -e \"try { require('child_process').execSync('npm run setup:wasm', {stdio: 'inherit'}); } catch(e) { console.log('WASM setup skipped (run npm run setup:wasm manually)'); }\""
  },
  "keywords": [
    "testing",
    "coverage",
    "security",
    "analysis",
    "cli",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@types/react": "^19.2.10",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "glob": "^13.0.0",
    "ink": "^6.6.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "js-yaml": "^4.1.1",
    "minimatch": "^10.1.1",
    "ora": "^8.0.1",
    "react": "^19.2.4",
    "web-tree-sitter": "^0.26.3",
    "yaml": "^2.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitest/coverage-v8": "^2.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "fast-check": "^4.5.3",
    "prettier": "^3.3.3",
    "tsup": "^8.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.5.3",
    "vitest": "^2.0.2"
  }
}
